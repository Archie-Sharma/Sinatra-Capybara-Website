<div class="container" style="margin-top:2.5%">
    <div class="row">
        <h3>Upload Track</h3>

        <form id="user_create_song" action="/music/create/song" method="POST" enctype="multipart/form-data">
            <label>Materialize File Input</label>
            <div class="file-field input-field">
                <div class="btn">
                    <span>Browse</span>
                    <input type="file" name="file" required>
                </div>
                <div class="file-path-wrapper">
                    <input class="file-path validate" type="text" placeholder="Upload file">
                </div>
            </div>

            <div class="input-field">
                <i class="material-icons prefix"></i>
                <input placeholder="Title" type="text" name="title" required>
                <label>Title</label>
            </div>

            <div class="input-field">
                <i class="material-icons prefix"></i>
                <input placeholder="Description" type="text" name="description" required>
                <label>Description your track</label>
            </div>

            <div class="input-field">
                <i class="material-icons prefix"></i>
                <input id="autocomplete-genre" placeholder="Drum and Bass..." type="text" name="genre" required>
                <label>Genre</label>
            </div>
            <label>Materialize Select</label>
            <select name="type" required>
              <option selected="selected" value="original">original</option>
              <option value="remix">remix</option>
              <option value="live">live</option>
              <option value="recording">recording</option>
              <option value="work">work</option>
              <option value="effect">effect</option>
              <option value="other">other</option>
            </select <center>
            <label>license</label>
            <select class="" name="license" required>
                <option selected="selected" value="creative_commons">All right reserved</option>
                <option value="all_right_reserved">Creative Commons</option>
            </select>
            <button class="btn waves-effect waves-light" type="submit" name="action">Upload Track
                <i class="material-icons right">send</i>
            </button>
        </center>

    </form>
</div>
</div>

